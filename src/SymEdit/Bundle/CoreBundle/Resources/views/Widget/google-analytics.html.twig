{% set enabled = symedit_settings_get('webmaster.google_analytics_enabled') %}
{% set code = symedit_settings_get('webmaster.google_analytics') %}

{% if code is not empty and enabled %}
<script type="text/javascript">
  var _gaq = _gaq || [];
  _gaq.push(['_setAccount', '{{ symedit_settings_get('webmaster.google_analytics') }}']);
  _gaq.push(['_trackPageview']);

  (function() {
    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
  })();
</script>
{% elseif enabled and code is empty %}
  Google Analytics enabled, but code not set. 
{% endif %}