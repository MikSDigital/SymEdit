<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="symedit_seo.event_listener.subject.class">SymEdit\Bundle\SeoBundle\EventListener\SubjectAnnotationSubscriber</parameter>
    </parameters>

    <services>
        <service id="symedit_seo.event_listener.annotation_subject" class="%symedit_seo.event_listener.subject.class%">
            <argument type="service" id="symedit_seo.seo_manager" />
            <argument type="service" id="annotation_reader" />
            <tag name="kernel.event_subscriber" />
        </service>
    </services>
</container>
