<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="symedit_settings.shortcode.setting.class">SymEdit\Bundle\SettingsBundle\Shortcode\SettingShortcode</parameter>
    </parameters>

    <services>
        <!-- Link -->
        <service id="symedit_settings.shortcode.setting" class="%symedit_settings.shortcode.setting.class%" public="false">
            <argument type="service" id="symedit_settings.settings" />
            <tag name="symedit_shortcode.shortcode" alias="setting" />
        </service>
    </services>
</container>
