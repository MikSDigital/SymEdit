{% extends '@SymEdit/Page/base.html.twig' %}

{% block heading %}
<h1>{% block blog_title %}{{ Page.tagline }}{% endblock %}</h1>
{% endblock %}

{% block content %}
<div id="blog-content">
    <div class="row">
        <div class="span9">
            {% block blog_content %}
                {% include '@SymEdit/Blog/list.html.twig' %}
            {% endblock %}
            {% if Pages is defined and Pages.num > 1 %}
                {% include '@SymEdit/Blog/paginate.html.twig' %}
            {% endif %}
        </div> 
        <div class="span3">
        {% block sidebar %}
            {% include '@SymEdit/CMS/sidebar.html.twig' %}
        {% endblock %}
        </div>
    </div>
</div>
{% endblock %}