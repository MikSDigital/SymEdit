{% extends 'IsometriksSymEditBundle::base.html.twig' %}

{% block title %}Users &raquo; {{ entity.username }} &raquo; Edit{% endblock %}

{% block content %}

<form class="form-horizontal" action="{{ path('admin_user_update', {id:entity.id}) }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <div class="form-actions">
        <button class="btn btn-primary" type="submit">Update User</button>
    </div>
</form>

{% endblock %}